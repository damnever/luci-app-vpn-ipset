config general
    option enabled '0'
    option ipset_name 'WG0_OVERWALL'
    option interface '_nil_'
    list ip_addresses '8.8.8.8'


config dnsmasq_ipset
    list dns_servers '8.8.8.8'
    list domains 'google.com'
    list domains 'feedly.com'
    list domains 'golang.org'
    list domains 'githubusercontent.com'
    list domains 'news.ycombinator.com'
    list domains 'stackexchange.com'
    list gfwlist_urls 'https://pagure.io/gfwlist/raw/master/f/gfwlist.txt'
